	<parameter name="factura" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     factura.`num_factura` AS factura_num_factura,
     factura.`nombre_emp` AS factura_nombre_emp,
     factura.`iddetalle` AS factura_iddetalle,
     factura.`apellido_emp` AS factura_apellido_emp,
     factura.`id_cli` AS factura_id_cli,
     factura.`nombre_cli` AS factura_nombre_cli,
     factura.`apellido_cli` AS factura_apellido_cli,
     factura.`fecha` AS factura_fecha,
     factura.`total` AS factura_total,
     detalle_factura.`iddetalle_factura` AS detalle_factura_iddetalle_factura,
     detalle_factura.`id_factura` AS detalle_factura_id_factura,
     detalle_factura.`id_prd` AS detalle_factura_id_prd,
     detalle_factura.`nombrepr` AS detalle_factura_nombrepr,
     detalle_factura.`precio` AS detalle_factura_precio,
     detalle_factura.`cantidad` AS detalle_factura_cantidad,
     ( detalle_factura.`cantidad` * detalle_factura.`precio`)AS importe,
     (0.18 * factura.`total`) as ITBIS
    
FROM
     `ferreteria_nyt`.`factura` factura,
     `ferreteria_nyt`.`detalle_factura` detalle_factura
WHERE  factura.iddetalle = $P{factura} AND
detalle_factura.id_factura= $P{factura}]]>
	</queryString>
